  "license": "Apache-2.0",
  "scripts": {
    "precommit": "if test -f precommit.hook ; then npm run test && npm run lint-staged; fi",
    "test": "mocha --use_strict --recursive test",
    "test:watch": "_mocha --watch --use_strict --recursive test",
    "test:unit": "mocha --use_strict --recursive test/unit",
    "test:integration": "mocha --use_strict --timeout 3000 --recursive test/integration",
    "cover:unit": "nyc mocha --recursive test/unit",
    "cover:integration": "nyc mocha --use_strict --timeout 3000 --recursive test/integration",
    "cover": "nyc mocha --use_strict --timeout 3000 --recursive test/integration --recursive test/unit",
    "lint": "eslint --ignore-path ../.gitignore --ext .js .",
    "lint-staged": "lint-staged",
    "check": "nsp check"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.2.0",
    "@types/sinon": "^5.0.0",
    "bunyan": "^1.8.12",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "mocha": "^5.2.0",
    "nsp": "^3.1.0",
    "nyc": "^11.3.0",
    "sinon": "^5.0.10",
    "proxyquire": "^2.0.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "needle": "^2.2.1",
    "urlencode": "^1.1.0",
    "jsonpath": "^1.0.0"
  }
}
